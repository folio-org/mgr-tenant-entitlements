{
  "request": {
    "method": "GET",
    "url": "/entitlements",
    "headers": {
      "X-Okapi-Permissions-Required": {
        "equalTo": "mgr-tenant-entitlements.entitlements.item.post"
      },
      "x-okapi-token": {
        "matches": ".+"
      },
      "X-Okapi-Tenant": {
        "equalTo": "supertenant"
      },
      "X-Okapi-Url": {
        "matches": ".*"
      }
    }
  },
  "response": {
    "status": 403
  }
}
