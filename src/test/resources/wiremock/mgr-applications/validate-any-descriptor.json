{
  "request": {
    "method": "POST",
    "urlPath": "/applications/validate",
    "headers": {
      "content-type": {
        "equalTo": "application/json"
      },
      "x-okapi-token": {
        "matches": ".+"
      }
    }
  },
  "response": {
    "status": 204,
    "headers": {
      "Content-Type": "application/json"
    }
  }
}
